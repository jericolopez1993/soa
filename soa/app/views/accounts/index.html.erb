<p id="notice"><%= notice %></p>

<h1>Listing Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Id client</th>
      <th>Yr</th>
      <th>Mon</th>
      <th>Day</th>
      <th>Po</th>
      <th>Re</th>
      <th>Sold</th>
      <th>Price</th>
      <th>Tamount</th>
      <th>Tcash</th>
      <th>Bal</th>
      <th>Paid</th>
      <th>Rsnum</th>
      <th>Prnum</th>
      <th>Addbal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.id_client %></td>
        <td><%= account.yr %></td>
        <td><%= account.mon %></td>
        <td><%= account.day %></td>
        <td><%= account.po %></td>
        <td><%= account.re %></td>
        <td><%= account.sold %></td>
        <td><%= account.price %></td>
        <td><%= account.tamount %></td>
        <td><%= account.tcash %></td>
        <td><%= account.bal %></td>
        <td><%= account.paid %></td>
        <td><%= account.rsnum %></td>
        <td><%= account.prnum %></td>
        <td><%= account.addbal %></td>
        <td><%= link_to 'Show', account %></td>
        <td><%= link_to 'Edit', edit_account_path(account) %></td>
        <td><%= link_to 'Destroy', account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account', new_account_path %>
